{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "proxy": "http://localhost:8080",
  "scripts": {
    "start": "node ./bin/www",
    "db:reset": "node db/scripts/resetdb.js",
    "server": "nodemon ./bin/www",
